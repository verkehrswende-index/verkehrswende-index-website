{
  "private": true,
  "name": "verkehrswindex-website",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "parcel ${PARCEL_BUILD_COMMAND:-build} src/index.html",
    "dev": "PARCEL_BUILD_COMMAND=serve npm run build",
    "lint": "eslint src",
    "test": "nodejs --experimental-vm-modules node_modules/.bin/jest --config=src/jest.config.js",
    "prettify": "prettier --write src"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "chroma-js": "^2.1.1",
    "font-awesome": "^4.7.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.5.2",
    "react-circular-progressbar": "^2.0.4",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-table": "^7.6.3"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.0-nightly.643",
    "eslint": "^7.24.0",
    "eslint-plugin-react": "^7.23.2",
    "jest": "^26.6.3",
    "npm": "^7.8.0",
    "parcel": "^2.0.0-nightly.641",
    "prettier": "2.2.1",
    "sass": "^1.32.8",
    "serve-static": "^1.14.1"
  }
}
